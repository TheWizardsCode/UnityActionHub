  m_Script: {fileID: 11500000, guid: a1f3a191fd1f7494eb7eb879d9e9e122, type: 3}
  m_Name: Add Auto Actions
  m_EditorClassIdentifier: 
  m_DisplayName: Add Auto Actions
  m_Description: "When an item is selected in the project window add it to a list
    of automatically created action items. The most recent 10 (or so) will be displayed,
    while 100 will be retained in memory and can be searched for. These can then
    include optimized actions for each item. For example:\n\n- [ ] scenes create
    a scene item, but within the auto's (where it will roll off if not used again)\n-
    [ ] prefabs offer the option to add it to the scene or to open the prefab. \n-
    [ ] items within a scene will ping the prefab (if there is one), focus on the
    item in the scene view\n- [ ] If it is an audio file it can be played\n"
  m_Category: {fileID: 11400000}
  m_Priority: 100
  m_IsComplete: 0
